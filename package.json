    "backend",
    "nodejs"
  ],
  "author": "Farid Alfernass",
  "license": "(ISC OR GPL-3.0)",
  "homepage": "https://gitlab.com/assesment-project/jinbei.git#readme",
  "dependencies": {
    "@elastic/ecs-winston-format": "^1.5.2",
    "aws-sdk": "^2.304.0",
    "azure-storage": "^2.10.3",
    "confidence": "^5.0.1",
    "dateformat": "^4.4.1",
    "dd-trace": "^3.8.0",
    "dotenv": "^16.0.3",
    "elastic-apm-node": "^3.46.0",
    "hot-shots": "^9.0.0",
    "jsonwebtoken": "^9.0.0",
    "passport": "0.7.0",
    "passport-http": "0.3.0",
