    "all": true,
    "cache": true,
    "report-dir": "./coverage"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "pre-commit": "^1.2.2",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.14.0",
